{
  "name": "psittacus",
  "version": "0.1.1",
  "description": "psittacus is a simple library to recorder audio in browser",
  "main": "dist/psittacus.js",
  "types": "dist/psittacus.d.ts",
  "directories": {
    "example": "example"
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "dev": "webpack-dev-server --config webpack-dev.config.js",
    "build:dev": "webpack --config webpack-dev.config.js",
    "build:prod": "webpack --config webpack-prod.config.js",
    "test": "npm run build:prod && karma start karma.conf.js",
    "publish": "npm run build:prod && np"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qiurenbo/psittacus.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/qiurenbo/psittacus/issues"
  },
  "homepage": "https://github.com/qiurenbo/psittacus#readme",
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/runtime": "^7.11.2",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "jasmine-core": "^3.6.0",
    "karma": "^5.1.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-jasmine": "^3.3.1",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^4.0.2",
    "np": "^6.4.0",
    "webpack": "^4.44.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
